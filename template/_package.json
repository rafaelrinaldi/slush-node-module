{
  "name": "<%= moduleName %>",
  "version": "1.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= authorGitHub %>/<%= moduleName %>",
  "bugs": {
    "url": "https://github.com/<%= authorGitHub %>/<%= moduleName %>/issues"
  },
  "homepage": "https://github.com/<%= authorGitHub %>/<%= moduleName %>#readme",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorHumanizedUrl %>"
  },<% if (hasCli) { %>
  "bin": {
    "<%= moduleName %>": "./bin/<%= moduleName %>"
  },<% } %>
  "engines": {
    <% if (hasCli) { %>"node": ">=0.10.0"<% } else { %>"node": ">=4"<% } %>
  },
  "xo": {
    <% if (hasCli) { %>"esnext": true,<% } %>
    "space": true,
    "rules": {
      "space-before-function-paren": [2, "never"]
    }
  },
  "scripts": {
    "lint": "xo",
    "test": "npm run lint && tape test.js | tap-spec"
  },
  "keywords": [
<%= moduleKeywords %>
  ],<% if (hasCli) { %>
  "dependencies": {
    "minimist": "^1.2.0"
  },<% } %>
  "devDependencies": {
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "xo": "^0.12.1"
  }
}
