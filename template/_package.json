{
  "name": "<%= moduleName %>",
  "version": "1.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= authorGitHub %>/<%= moduleName %>",
  "bugs": {
    "url": "https://github.com/<%= authorGitHub %>/<%= moduleName %>/issues"
  },
  "homepage": "https://github.com/<%= authorGitHub %>/<%= moduleName %>#readme",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },<% if (hasCli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "lint": "eslint",
    "test": "npm run lint && tape test.js",
    "postversion": "git push origin master --tags && npm publish"
  },
  "files": [
    "index.js"<% if (hasCli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [
<%= moduleKeywords %>
  ],
  "dependencies": {<% if (hasCli) { %>
    "minimist": "^1.2.0",
    "multiline": "^1.0.2"
  <% } %>},
  "devDependencies": {
    "eslint": "^1.2.1",
    "eslint-plugin-no-use-extend-native": "*",
    "tape": "^4.2.0"
  }
}
